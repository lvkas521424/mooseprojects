{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "gfortran",
            "args": [
                "-g",
                "-fopenmp",
                "min.f90",
                "-o",
                "min"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "options": {
                "cwd": "${workspaceFolder}"
            }
        }
    ],
    "[moose]": {
        "outline.showProperties": false,
        "outline.showStrings": false
    },
    "files.associations": {
        "BurnupStepManager.C": "cpp",
        "GlobalBurnupStep.C": "cpp",
        "ReactorCouplingUserObject.C": "cpp"
    }
}
